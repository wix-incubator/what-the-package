{
  "name": "what-the-package",
  "version": "1.1.1",
  "main": "./src/main.js",
  "author": "javinor <javinor@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/wix-incubator/what-the-package",
  "scripts": {
    "lint": "eslint . --fix",
    "test": "jest",
    "precommit": "lint-staged",
    "prepush": "npm test"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint --",
      "git add"
    ]
  },
  "bin": {
    "wtp": "./src/cli.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "logLevel": "verbose"
  },
  "devDependencies": {
    "eslint": "^5.2.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "flow-bin": "^0.77.0",
    "husky": "^0.14.3",
    "jest": "^23.4.1",
    "lint-staged": "^7.2.0",
    "prettier": "1.13.7",
    "wnpm-ci": "^6.2.54"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "ora": "^3.0.0",
    "rimraf": "^2.6.2",
    "semver": "^5.5.0",
    "text-table": "^0.2.0",
    "yargs": "^12.0.1"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "jest": true,
      "node": true
    },
    "extends": [
      "plugin:prettier/recommended",
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2017
    }
  },
  "eslintIgnore": [
    "flow-typed"
  ],
  "prettier": {
    "semi": false
  },
  "jest": {
    "bail": true,
    "silent": false,
    "verbose": true,
    "testURL": "http://localhost"
  }
}
